nNOM ivfin1     	      	;; small test suite only intrans verbs, assumes fin comp
nNOM ivfin2
*ivfin1 nNOM   	      	;; assumes SVO
*iv nNOM			;; tests uninflected verb form
*aux nNOM ivfin1		;; tests uninflected aux 
*auxfin1 nNOM iv 	      	;; tests uninflected verb in comp
*auxfin1 nNOM ivfin1     	;; tests fin aux w/ fin comp
*nNOM auxfin1 ivfin1   	;; tests order
*auxfin1 nNOM ivfin2   	;; tests non-agreement in tense
auxfin1 nNOM ivnf1    	;; assumes fin aux requires a nf1 comp form
auxfin2 nNOM ivnf1
*auxfin1 nNOM ivnf2	;; tests aux w/ wrong nf form
pauxfin1 nNOM ivnf2   	;; assumes paux requires a nf2 comp form
pauxfin2 nNOM ivnf2
*ivnf2 nNOM pauxfin1	;; tests order
*pauxfin1 nNOM ivnf1 	;; tests wrong nf form
*paux nNOM ivfin1		;; tests fin comp
