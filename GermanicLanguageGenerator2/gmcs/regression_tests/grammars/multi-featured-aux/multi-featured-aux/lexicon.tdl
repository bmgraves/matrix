;;; Nouns

n1 := noun1-noun-lex &
  [ STEM < "n1" >,
    SYNSEM.LKEYS.KEYREL.PRED "_n1_n_rel" ].

n2 := noun2-noun-lex &
  [ STEM < "n2" >,
    SYNSEM.LKEYS.KEYREL.PRED "_n2_n_rel" ].

;;; Verbs

iv := ivnofeat-verb-lex &
  [ STEM < "iv" >,
    SYNSEM.LKEYS.KEYREL.PRED "_iv_v_rel" ].

tv := verb2-verb-lex &
  [ STEM < "tv" >,
    SYNSEM.LKEYS.KEYREL.PRED "_tv_v_rel" ].

ivf := ivfeat-verb-lex &
  [ STEM < "ivf" >,
    SYNSEM.LKEYS.KEYREL.PRED "_ivf_v_rel" ].

;;; Auxiliaries

auxa := t1a-aux-lex &
  [ STEM < "auxa" >,
    SYNSEM.LKEYS.KEYREL.PRED "_aux1_v_rel" ].

auxb := t1b-aux-lex &
  [ STEM < "auxb" >,
    SYNSEM.LKEYS.KEYREL.PRED "_auxb_v_rel" ].
