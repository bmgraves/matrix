;;; Nouns

n1 := noun1-noun-lex &
  [ STEM < "n1" >,
    SYNSEM.LKEYS.KEYREL.PRED "_n1_n_rel" ].

;;; Verbs

v1a := verb1-verb-lex &
  [ STEM < "v1a" >,
    SYNSEM.LKEYS.KEYREL.PRED "_v1a_v_rel" ].

v2a := verb2-verb-lex &
  [ STEM < "v2a" >,
    SYNSEM.LKEYS.KEYREL.PRED "_v2a_v_rel" ].

;;; Auxiliaries

nopaux := aux1-aux-lex &
  [ STEM < "nopaux" > ].

paux := aux2-aux-lex &
  [ STEM < "paux" >,
    SYNSEM.LKEYS.KEYREL.PRED "_paux_v_rel" ].
