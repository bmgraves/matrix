;;; Nouns

n1 := noun1-noun-lex &
  [ STEM < "n1" >,
    SYNSEM.LKEYS.KEYREL.PRED "_n1_n_rel" ].

n2 := noun2-noun-lex &
  [ STEM < "n2" >,
    SYNSEM.LKEYS.KEYREL.PRED "_n2_n_rel" ].

;;; Verbs

aiv := avoirintran-verb-lex &
  [ STEM < "aiv" >,
    SYNSEM.LKEYS.KEYREL.PRED "_aiv_v_rel" ].

atv := avoirtrans-verb-lex &
  [ STEM < "atv" >,
    SYNSEM.LKEYS.KEYREL.PRED "_atv_v_rel" ].

eiv := etreintrans-verb-lex &
  [ STEM < "eiv" >,
    SYNSEM.LKEYS.KEYREL.PRED "_eiv_v_rel" ].

etv := etretrans-verb-lex &
  [ STEM < "etv" >,
    SYNSEM.LKEYS.KEYREL.PRED "_etv_v_rel" ].

;;; Auxiliaries

avoir := atype-aux-lex &
  [ STEM < "avoir" > ].

etre := etype-aux-lex &
  [ STEM < "etre" > ].
