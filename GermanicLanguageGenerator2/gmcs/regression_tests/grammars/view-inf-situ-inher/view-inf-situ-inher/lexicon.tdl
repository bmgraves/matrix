;;; Nouns

n1 := noun1-noun-lex &
  [ STEM < "n1" >,
    SYNSEM.LKEYS.KEYREL.PRED "_n1_n_rel" ].

n2 := noun2-noun-lex &
  [ STEM < "n2" >,
    SYNSEM.LKEYS.KEYREL.PRED "_n2_n_rel" ].

;;; Verbs

one := typeone-verb-lex &
  [ STEM < "one" >,
    SYNSEM.LKEYS.KEYREL.PRED "_one_v_rel" ].

two := typetwo-verb-lex &
  [ STEM < "two" >,
    SYNSEM.LKEYS.KEYREL.PRED "_two_v_rel" ].

tv := trans-verb-lex &
  [ STEM < "tv" >,
    SYNSEM.LKEYS.KEYREL.PRED "_tv_v_rel" ].
