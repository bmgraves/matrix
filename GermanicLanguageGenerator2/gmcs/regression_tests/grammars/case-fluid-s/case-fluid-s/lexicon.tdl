;;; Nouns

n1 := noun1-noun-lex &
  [ STEM < "n1" >,
    SYNSEM.LKEYS.KEYREL.PRED "_n1_n_rel" ].

n2 := noun2-noun-lex &
  [ STEM < "n2" >,
    SYNSEM.LKEYS.KEYREL.PRED "_n2_n_rel" ].

;;; Verbs

iv-o := verb1-verb-lex &
  [ STEM < "iv-o" >,
    SYNSEM.LKEYS.KEYREL.PRED "_iv-o_v_rel" ].

iv-a := verb2-verb-lex &
  [ STEM < "iv-a" >,
    SYNSEM.LKEYS.KEYREL.PRED "_iv-a_v_rel" ].

iv := verb3-verb-lex &
  [ STEM < "iv" >,
    SYNSEM.LKEYS.KEYREL.PRED "_iv_v_rel" ].

tv := verb4-verb-lex &
  [ STEM < "tv" >,
    SYNSEM.LKEYS.KEYREL.PRED "_tv_v_rel" ].
