;;; -*- Mode: TDL; Coding: utf-8 -*-
;;; Nouns

babae := common-noun-lex &
  [ STEM < "babae" >,
    SYNSEM.LKEYS.KEYREL.PRED "_babae_n_rel" ].

lalaki := common-noun-lex &
  [ STEM < "lalaki" >,
    SYNSEM.LKEYS.KEYREL.PRED "_lalaki_n_rel" ].

;;; Adjectives

;;; Case-marking adpositions

focus-marker := case-marking-adp-lex &
  [ STEM < "ang" >,
    SYNSEM.LOCAL [ CONT [ HOOK [ ICONS-KEY.IARG1 #clause,
                                 CLAUSE-KEY #clause ],
                          ICONS <!  !> ],
                   CAT.HEAD.CASE foc ] ].

a_case%2C%20o_case-marker := case-marking-adp-lex &
  [ STEM < "ng" >,
    SYNSEM.LOCAL [ CONT [ HOOK [ ICONS-KEY.IARG1 #clause,
                                 CLAUSE-KEY #clause ],
                          ICONS <!  !> ],
                   CAT.HEAD.CASE non-foc ] ].

;;; Verbs

tulog := verb1-verb-lex &
  [ STEM < "tulog" >,
    SYNSEM.LKEYS.KEYREL.PRED "_tulog_v_rel" ].

kita := verb2-verb-lex &
  [ STEM < "kita" >,
    SYNSEM.LKEYS.KEYREL.PRED "_kita_v_rel" ].
