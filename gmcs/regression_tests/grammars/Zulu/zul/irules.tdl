;;; Inflecting Lexical Rule Instances

C1-prefix :=
%prefix (* c1-)
C1-lex-rule.

C9-prefix :=
%prefix (* c9-)
C9-lex-rule.

OC1-prefix :=
%prefix (* oc1-)
OC1-lex-rule.

OC9-prefix :=
%prefix (* oc9-)
OC9-lex-rule.

fut-tense-prefix :=
%prefix (* fut-)
fut-tense-lex-rule.

pres-tense-prefix :=
%prefix (* pres-)
pres-tense-lex-rule.

SC1-prefix :=
%prefix (* sc1-)
SC1-lex-rule.

SC9-prefix :=
%prefix (* sc9-)
SC9-lex-rule.

default-FV-suffix :=
%suffix (* -a)
default-FV-lex-rule.

neg-FV-suffix :=
%suffix (* -e)
neg-FV-lex-rule.

negation-prefix :=
%prefix (* neg-)
negation-lex-rule.
