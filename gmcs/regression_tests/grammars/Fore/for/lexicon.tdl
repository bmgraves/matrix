    SYNSEM.LKEYS.KEYREL.PRED "_yaga:_n_rel" ].

naninta%3A := noun6-noun-lex &
  [ STEM < "naninta:" >,
    SYNSEM.LKEYS.KEYREL.PRED "_naninta:_n_rel" ].

