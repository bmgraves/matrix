;;; Inflecting Lexical Rule Instances

masculine_noun-suffix :=
%suffix (* m)
masculine_noun-lex-rule.

feminine_noun-suffix :=
%suffix (* f)
feminine_noun-lex-rule.

masculine_adj-suffix :=
%suffix (* m)
masculine_adj-lex-rule.

feminine_adj-suffix :=
%suffix (* f)
feminine_adj-lex-rule.
