;;; -*- Mode: TDL; Coding: utf-8 -*-
;;; Nouns

n := noun-noun-lex &
  [ STEM < "n" >,
    SYNSEM.LKEYS.KEYREL.PRED "_n_n_rel" ].

;;; Adjectives

adj := adj-adj-lex &
  [ STEM < "adj" >,
    SYNSEM.LKEYS.KEYREL.PRED "_adj_a_rel" ].

;;; Verbs

tv := tv-verb-lex &
  [ STEM < "tv" >,
    SYNSEM.LKEYS.KEYREL.PRED "_tv_v_rel" ].

iv := iv-verb-lex &
  [ STEM < "iv" >,
    SYNSEM.LKEYS.KEYREL.PRED "_iv_v_rel" ].

;;; Determiners

det := det-determiner-lex &
  [ STEM < "det" >,
    SYNSEM.LKEYS.KEYREL.PRED "_det_q_rel" ].
